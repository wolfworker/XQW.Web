    <script>

        $(function () {
            //加载二级分类列表
            loadCategory()
        });

        function loadCategory() {
            $.ajax({
                url: "../Category/GetAllBCategoryList",    //请求的url地址
                dataType: "json",   //返回格式为json
                async: true, //请求是否异步，默认为异步，这也是ajax重要特性
                type: "get",   //请求方式
                success: function (result) {
                    //请求成功时处理
                    var htmlstr = '';
                    $.each(result, function (i, item) {
                        htmlstr += '<li><a href="/Category/BIndex?bcategoryid=' + item.BCategoryID + '" >' + item.BCategoryName + '</a></li>';
                    });
                    $(".header-menu").append(htmlstr);
                    $(".mobile-menu-nav").append(htmlstr);
                }
            });
        }
